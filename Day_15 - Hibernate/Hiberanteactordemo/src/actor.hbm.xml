<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<class table="actor" name="com.highradius.hiberante.pojo.Actor">
	<id name="id" column="actor_id" type="int">
	<generator class="identity"></generator>
	</id>
<property name="firstName" column="first_name" type="string"></property>
<property name="lastName" column="last_name" type="string"></property>
</class>
</hibernate-mapping>

